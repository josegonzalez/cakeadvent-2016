{
    "name": "your-name-here/PhotoPostType",
    "description": "PhotoPostType plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.5.0 <4.0.0",
        "admad/cakephp-glide": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "PhotoPostType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhotoPostType\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
